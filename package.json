{
  "name": "react-redux-test",
  "version": "1.0.0",
  "description": "A project developed with the intention of learning react with redux",
  "main": "main.js",
  "scripts": {
    "dev-build": "jspm bundle main.js -wid",
    "build": "jspm bundle main.js build.js --minify",
    "lint": "eslint src/* || exit 0"
  },
  "keywords": [],
  "author": "sahiljain112@gmail.com",
  "license": "ISC",
  "devDependencies": {
    "eslint": "^3.4.0",
    "eslint-config-airbnb": "^10.0.1",
    "eslint-plugin-import": "^1.14.0",
    "eslint-plugin-jsx-a11y": "^2.2.1",
    "eslint-plugin-react": "^6.2.0",
    "jspm": "^0.17.0-beta.28"
  },
  "jspm": {
    "name": "react-redux-test",
    "main": "main.js",
    "dependencies": {
      "eslint-plugin-react": "npm:eslint-plugin-react@^6.2.0",
      "immutable": "npm:immutable@^3.8.1",
      "react-dom": "npm:react-dom@^15.3.1",
      "react-redux": "npm:react-redux@^4.4.5"
    },
    "devDependencies": {
      "babel-eslint": "npm:babel-eslint@^6.1.2",
      "babel-plugin-transform-object-assign": "npm:babel-plugin-transform-object-assign@^6.8.0",
      "babel-plugin-transform-object-rest-spread": "npm:babel-plugin-transform-object-rest-spread@^6.8.0",
      "babel-plugin-transform-react-jsx": "npm:babel-plugin-transform-react-jsx@^6.8.0",
      "babel-preset-react": "npm:babel-preset-react@^6.11.1",
      "babel-preset-stage-0": "npm:babel-preset-stage-0@^6.5.0",
      "core-js": "npm:core-js@^2.4.1",
      "plugin-babel": "npm:systemjs-plugin-babel@^0.0.13"
    },
    "peerDependencies": {
      "assert": "github:jspm/nodelibs-assert@^0.2.0-alpha",
      "buffer": "github:jspm/nodelibs-buffer@^0.2.0-alpha",
      "child_process": "github:jspm/nodelibs-child_process@^0.2.0-alpha",
      "constants": "github:jspm/nodelibs-constants@^0.2.0-alpha",
      "crypto": "github:jspm/nodelibs-crypto@^0.2.0-alpha",
      "domain": "github:jspm/nodelibs-domain@^0.2.0-alpha",
      "eslint": "npm:eslint@^3.4.0",
      "events": "github:jspm/nodelibs-events@^0.2.0-alpha",
      "fs": "github:jspm/nodelibs-fs@^0.2.0-alpha",
      "http": "github:jspm/nodelibs-http@^0.2.0-alpha",
      "https": "github:jspm/nodelibs-https@^0.2.0-alpha",
      "module": "github:jspm/nodelibs-module@^0.2.0-alpha",
      "os": "github:jspm/nodelibs-os@^0.2.0-alpha",
      "path": "github:jspm/nodelibs-path@^0.2.0-alpha",
      "process": "github:jspm/nodelibs-process@^0.2.0-alpha",
      "react": "npm:react@^15.3.1",
      "readline": "github:jspm/nodelibs-readline@^0.2.0-alpha",
      "redux": "npm:redux@^3.0.0",
      "stream": "github:jspm/nodelibs-stream@^0.2.0-alpha",
      "string_decoder": "github:jspm/nodelibs-string_decoder@^0.2.0-alpha",
      "tty": "github:jspm/nodelibs-tty@^0.2.0-alpha",
      "url": "github:jspm/nodelibs-url@^0.2.0-alpha",
      "util": "github:jspm/nodelibs-util@^0.2.0-alpha",
      "vm": "github:jspm/nodelibs-vm@^0.2.0-alpha",
      "zlib": "github:jspm/nodelibs-zlib@^0.2.0-alpha"
    },
    "overrides": {
      "npm:bluebird@3.4.6": {
        "meta": {
          "js/browser/bluebird.js": {
            "format": "global"
          },
          "js/browser/bluebird.min.js": {
            "format": "global"
          }
        }
      },
      "npm:browserify-zlib@0.1.4": {
        "dependencies": {
          "readable-stream": "^2.0.2",
          "pako": "~0.2.0"
        },
        "map": {
          "_stream_transform": "readable-stream/transform"
        }
      },
      "npm:debug@2.2.0": {
        "main": "browser.js",
        "jspmNodeConversion": false,
        "format": "cjs",
        "map": {
          "./browser.js": {
            "node": "./node.js"
          },
          "fs": "@node/fs",
          "net": "@node/net",
          "tty": "@node/tty",
          "util": "@node/util"
        }
      },
      "npm:esprima@2.7.3": {
        "jspmNodeConversion": false
      },
      "npm:inherits@2.0.3": {
        "ignore": [
          "test.js"
        ]
      },
      "npm:lodash@4.15.0": {
        "map": {
          "buffer": "@empty",
          "process": "@empty"
        }
      },
      "npm:ms@0.7.1": {
        "jspmNodeConversion": false,
        "format": "cjs"
      }
    }
  }
}
